  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net7.0-ios16.1</TargetFramework>
    <SupportedOSPlatformVersion>10.0</SupportedOSPlatformVersion>
    <ProvisioningType>manual</ProvisioningType>
    <Nullable>enable</Nullable>
    <RuntimeIdentifier>iossimulator-x64</RuntimeIdentifier>
    <AssemblyName>TableConverter</AssemblyName>
    <Title>TableConverter</Title>
    <Authors>MrT Stephens</Authors>
    
    <!-- These properties need to be set in order to run on a real iDevice -->
