  <ItemGroup>
    <PackageReference Include="Avalonia.iOS" Version="11.0.6" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\TableConverter\TableConverter.csproj" />
  </ItemGroup>
</Project>
