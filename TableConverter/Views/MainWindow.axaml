<sukiUi:SukiWindow xmlns="https://github.com/avaloniaui"
                   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                   xmlns:vm="using:TableConverter.ViewModels"
                   xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                   xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                   xmlns:sukiUi="clr-namespace:SukiUI.Controls;assembly=SukiUI"
                   xmlns:views="clr-namespace:TableConverter.Views"
                   x:Class="TableConverter.Views.MainWindow"
                   Icon="/Assets/avalonia-logo.ico"
                   Title="TableConverter"
                   BackgroundAnimationEnabled="True"
                   MinHeight="400"
                   MinWidth="400"
                   mc:Ignorable="d" 
                   d:DesignWidth="800" 
                   d:DesignHeight="450">
  <sukiUi:SukiWindow.LogoContent>
    <PathIcon Data="{StaticResource TableEdit}" />
  </sukiUi:SukiWindow.LogoContent>
  <views:MainView />
</sukiUi:SukiWindow>
